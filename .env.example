PORT=<puertoDeBackend>

# Configuración de JWT
JWT_SECRET=<claveSecretaParaJWT>
JWT_EXPIRES_IN=<tiempoDeExpiracionDelToken>

# Configuración de Base de Datos
POSTGRES_USER=<usuarioPg>
POSTGRES_PASSWORD=<contraseñaPg>
POSTGRES_DB=<nombreBaseDeDatosPg>

# Configuración de PgAdmin
PGADMIN_EMAIL=<emailAdminPgadmin>
PGADMIN_PASSWORD=<contraseñaPgadmin>

# URL de Base de Datos para uso local (fuera de Docker)
DATABASE_URL=postgresql://<usuarioPg>:<contraseñaPg>@<host>:<puertoPg>/<nombreBaseDeDatosPg>
